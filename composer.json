{
  "name": "dsmithhayes/dsh-theme",
  "description": "Theme for davesmithhayes.com forked from the Timber Stater Theme.",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "name": "jarednova",
      "email": "jared@upstatement.com"
    },
    {
      "name": "Dave Smith-Hayes",
      "email": "me@davesmithhayes.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
    }
  },
  "require": {
    "wpackagist-plugin/timber-library": "1.2.*"
  },
  "autoload": {
    "psr-4": {
      "DshTheme\\": "src/"
    }
  }
}
